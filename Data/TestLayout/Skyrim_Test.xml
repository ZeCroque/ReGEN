<test name="Skyrim_Test" save_output="True">
	<socialgraph>Skyrim</socialgraph>
	<initializationrules>
		<initializationrule>Assassinate</initializationrule>
		<initializationrule>Break_Ally</initializationrule>
		<initializationrule>Fight</initializationrule>
		<initializationrule>Forge_Ally</initializationrule>
		<initializationrule>Give_Blackmail_Letter</initializationrule>
		<initializationrule>Give_Gift</initializationrule>
		<initializationrule>Murder</initializationrule>
		<initializationrule>Murder_Blackmailer</initializationrule>
		<initializationrule>Rebel</initializationrule>
	</initializationrules>
	<rewriterules>
	    <rewriterule>Ambush</rewriterule>
	    <rewriterule>Ambush_By_Hater</rewriterule>
	    <rewriterule>Caught</rewriterule>
	    <rewriterule>More_Fight</rewriterule>
	    <rewriterule>Spare</rewriterule>
	    <rewriterule>Stealth_Kill</rewriterule>
	</rewriterules>
	<numstoriestogenerate>10</numstoriestogenerate>
	<maxnumberofrewrites>100</maxnumberofrewrites>
	<metricstooptimize>
		<metric name="fights" weight="1" ></metric>
		<metric name="repetitivity" weight="1"></metric>
		<metric name="longest_path" weight="1"></metric>
		<metric name="shortest_path" weight="-1"></metric>
		<metric name="num_branches" weight="1"></metric>
	</metricstooptimize>
	<metricstoanalyze>
	</metricstoanalyze>
</test>
